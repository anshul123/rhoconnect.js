        <link href="rhoconnect-sencha/application.css" rel="stylesheet" type="text/css" />

      <% if 'development' == ENV['RACK_ENV'] %>
          <script type="text/javascript" language="javascript" src="../external/jquery/jquery-1.6.1.js"></script>
          <script type="text/javascript" language="javascript" src="../external/jquery/jquery.base64.js"></script>
      <% else %>
          <script type="text/javascript" language="javascript" src="../external/jquery/jquery-1.6.1.min.js"></script>
          <script type="text/javascript" language="javascript" src="../external/jquery/jquery.base64.min.js"></script>
