        <script type="text/javascript" language="javascript" src="rhoconnect-sencha/external/sencha/cardHistoryLayout.js"></script>

        <script type="text/javascript">
            var syncUrl = '';
            <% if 'development' == ENV['RACK_ENV'] %>
                // For cross-domain configuration
                syncUrl = 'http://rhohub-lars-692f63a7.rhosync.com/application';
                //syncUrl = 'http://192.168.0.21:9292/application';
                // For non-cross-domain use this settings:
                //syncUrl = window.location.protocol +'//' +window.location.host +'/application';
            <% else %>
                syncUrl = 'http://rhohub-lars-692f63a7.rhosync.com/application';
                //syncUrl = 'http://rhoconnect-js-server/application';
                //syncUrl = 'http://rhodes-store-server.heroku.com/application';
            <% end %>

            RhoConfig = {
                appName: 'rhoSenchaTouchSample',
                syncServer: syncUrl,
                logLevel: 'trace'
            };
